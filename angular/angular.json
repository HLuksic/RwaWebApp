{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projekti",
  "projects": {
    "DemoProjekt": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projekti/demo-projekt",
      "sourceRoot": "projekti/demo-projekt/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-projekt",
            "index": "projekti/demo-projekt/src/index.html",
            "main": "projekti/demo-projekt/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projekti/demo-projekt/tsconfig.app.json",
            "assets": [
              "projekti/demo-projekt/src/favicon.ico",
              "projekti/demo-projekt/src/assets"
            ],
            "styles": [
              "projekti/demo-projekt/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/vjezba_08/src/environments/environment.ts",
                  "with": "projects/vjezba_08/src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "DemoProjekt:build:production"
            },
            "development": {
              "browserTarget": "DemoProjekt:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DemoProjekt:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projekti/demo-projekt/tsconfig.spec.json",
            "assets": [
              "projekti/demo-projekt/src/favicon.ico",
              "projekti/demo-projekt/src/assets"
            ],
            "styles": [
              "projekti/demo-projekt/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "vjezba_08": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projekti/vjezba_08",
      "sourceRoot": "projekti/vjezba_08/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/vjezba_08",
            "index": "projekti/vjezba_08/src/index.html",
            "main": "projekti/vjezba_08/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projekti/vjezba_08/tsconfig.app.json",
            "assets": [
              "projekti/vjezba_08/src/favicon.ico",
              "projekti/vjezba_08/src/assets"
            ],
            "styles": [
              "projekti/vjezba_08/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
