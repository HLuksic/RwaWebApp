            },
            "development": {
              "browserTarget": "vjezba_08:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vjezba_08:build"
          }
        }
      }
    }
  }
}